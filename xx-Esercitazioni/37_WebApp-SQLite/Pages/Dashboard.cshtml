@page 
@model DashboardModel

@{
    ViewData["Title"] = "Dashboard";
}

<h1>@ViewData["Title"]</h1>
<hr>
<div class="row">
        <div class="col-md-4">
            @await Html.PartialAsync("ProdottiPiuCostosi",Model.ProdottiPiuCostosi)
        </div>
        <div class="col-md-4">
            @await Html.PartialAsync("ProdottiRecenti",Model.ProdottiRecenti)
        </div>
        <div class="col-md-4">
            @await Html.PartialAsync("ProdottiPerCategoria",Model.ProdottiPerCategoria)
        </div>
</div>